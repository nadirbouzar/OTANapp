<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Page_Stock">

    <TextView
        android:id="@+id/titleTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:padding="8dp"
        android:text="stocks"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="32dp" />

    <Button
        android:id="@+id/button_allemagne"
        android:layout_width="141dp"
        android:layout_height="50dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="7dp"
        android:text="allemagne"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewAllemagne" />

    <Button
        android:id="@+id/button_uk"
        android:layout_width="141dp"
        android:layout_height="50dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="32dp"
        android:text="uk"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <Button
        android:id="@+id/button_italie"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginStart="105dp"
        android:layout_marginEnd="105dp"
        android:layout_marginBottom="16dp"
        android:text="italie"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageViewAllemagne"
        android:layout_width="175dp"
        android:layout_height="123dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:contentDescription="imageallemagne"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_etatunis"
        app:srcCompat="@drawable/allemagne"
        tools:ignore="DuplicateIds" />

    <ImageView
        android:id="@+id/imageViewItalie"
        android:layout_width="195dp"
        android:layout_height="123dp"
        android:layout_marginStart="108dp"
        android:layout_marginEnd="108dp"
        android:contentDescription="imageitalie"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_allemagne"
        app:srcCompat="@drawable/italie" />

    <ImageView
        android:id="@+id/imageViewFrance"
        android:layout_width="175dp"
        android:layout_height="123dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="16dp"
        android:contentDescription="imagefrance"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titleTextView"
        app:srcCompat="@drawable/france" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="195dp"
        android:layout_height="150dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="70dp"
        android:contentDescription="imageeu"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titleTextView"
        app:srcCompat="@drawable/etats_unis" />

    <Button
        android:id="@+id/button_france"
        android:layout_width="141dp"
        android:layout_height="50dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="36dp"
        android:text="france"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewFrance" />

    <Button
        android:id="@+id/button_etatunis"
        android:layout_width="141dp"
        android:layout_height="50dp"
        android:layout_marginStart="36dp"
        android:text="etats_unis"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="175dp"
        android:layout_height="123dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="16dp"
        android:contentDescription="imageuk"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_france"
        app:srcCompat="@drawable/royaume_unis" />

</androidx.constraintlayout.widget.ConstraintLayout>